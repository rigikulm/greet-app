{
  "name": "greet-app",
  "version": "1.0.0",
  "description": "Sample Typescript lambda functions",
  "main": "index.js",
  "scripts": {
    "build": "webpack-cli",
    "clean": "rimraf .aws-sam .vscode coverage",
    "prebuild": "rimraf .aws-sam .vscode",
    "prewatch": "rimraf .aws-sam .vscode",
    "watch": "webpack-cli -w",
    "test": "jest",
    "startnet": "docker network create sam-local",
    "startdb": "docker run --network sam-local --name dynamodb -p 8000:8000 amazon/dynamodb-local",
    "createdb": "/bin/sh ./test/util/create-table.sh",
    "stagedb": "/bin/sh ./test/util/stage-db.sh",
    "dumpdb": "/bin/sh ./test/util/dump-db.sh",
    "startapi": "sam local start-api --docker-network sam-local",
    "deploy:dev": "sam deploy --config-file ./samconfig.toml --parameter-overrides Stage=dev",
    "deploy:prod": "sam deploy --config-file ./samconfig.toml --parameter-overrides Stage=prod"
  },
  "keywords": [
    "SAM",
    "Lambda",
    "Typescript",
    "Webpack",
    "Serverless"
  ],
  "author": "Chris Lemler <clemler@pm.me>",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.64",
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/webpack": "^4.41.25",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "aws-sam-webpack-plugin": "^0.7.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "aws-sdk": "^2.828.0"
  }
}
